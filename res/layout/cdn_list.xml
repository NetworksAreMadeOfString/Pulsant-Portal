<RelativeLayout android:id="@+id/RelativeLayout01" android:layout_width="fill_parent" android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android"><TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/CDNName" android:textStyle="bold" android:textSize="16px" android:text="CDN name"></TextView>



<LinearLayout android:layout_width="wrap_content" android:layout_below="@+id/CDNName" android:id="@+id/LinearLayout01" android:layout_height="wrap_content" android:layout_centerHorizontal="true">
<ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/EUCDNLabel" android:src="@drawable/cdn_eu"></ImageView>
<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="6px" android:text="0Gb" android:id="@+id/EMEAUsage"></TextView>

<ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/USCDNLabel" android:paddingLeft="20px" android:src="@drawable/cdn_us"></ImageView>
<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="6px" android:text="0Gb" android:id="@+id/USUsage"></TextView>

<ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/EUCDNLabel" android:paddingLeft="20px" android:src="@drawable/cdn_asia"></ImageView>
<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="6px" android:text="0Gb" android:id="@+id/AsiaUsage"></TextView>
</LinearLayout>


</RelativeLayout>
